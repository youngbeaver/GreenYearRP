{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [ "ESNext", "ES2019" ],
        "moduleResolution": "node",
        "module": "ESNext",
        "types": [ "@ragempcommunity/types-client" ],
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true
    },
    "include": [ "src" ]
}

